	},
	"files.exclude": {
		"**/*.meta": true
	},
	"editor.rename.enablePreview": false,
	"editor.hover.enabled": true,
	"editor.hover.sticky": false,
	"editor.lightbulb.enabled": false,
	"editor.parameterHints.enabled": true,
	"editor.renderFinalNewline": false,
	"editor.foldingHighlight": false,
	"editor.highlightActiveIndentGuide": false,
	"editor.semanticHighlighting.enabled": false,
	"scm.diffDecorations": "none",
	"window.titleBarStyle": "native",
	"git.decorations.enabled": false,
	"workbench.colorCustomizations": {
		"editor.background": "#47177c",
		"editorUnnecessaryCode.opacity": "#ffffff",
		"editorWidget.background": "#555555",
		"dropdown.background": "#ffffff",
		"dropdown.listBackground": "#555555",
		"dropdown.border": "#000000",
		"dropdown.foreground": "#000000",
		"list.highlightForeground": "#ffff55",
		"list.hoverBackground": "#aaaaaa",
		"pickerGroup.foreground": "#ffff55",
		"contrastActiveBorder": "#000000",
		"contrastBorder": "#000000",
		// "input.background": "#ffffff",
		// "input.border": "#0000aa",
		// "input.foreground": "#ffffff",
		// "input.placeholderForeground": "#000000",
	},
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": [
					"keyword",
					"keyword.operator",
					"storage.modifier",
					"entity.name.function",
					"constant.numeric",
					"constant.language",
					"keyword.control"
				],
				"settings": {
					"foreground": "#FFFFFF"
				}
			},
			{
				"scope": [
					"comment",
					"string"
				],
				"settings": {
					"foreground": "#ffff55"
				}
			},
		]
	},
	"workbench.editor.showTabs": false,
	"workbench.editor.showIcons": false,
	"window.zoomLevel": 2,
	"csharpfixformat.style.spaces.afterBracket": false,
	"csharpfixformat.style.spaces.beforeParenthesis": false,
	"csharpfixformat.style.spaces.insideEmptyBraces": false,
	"csharpfixformat.style.spaces.removeAfterCommandBeforeParenthesis": "true",
	"explorer.confirmDelete": false,
	"git.enabled": false,
	"python.linting.pylintEnabled": false,
	"python.linting.enabled": true,
	"python.linting.banditEnabled": true,
	"python.formatting.provider": "autopep8",
