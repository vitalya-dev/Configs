	"alt-editor.args": "+{line}:{column} {filename}",
    "alt-editor.binary": "C:\\Users\\Vitalya\\AppData\\Roaming\\emacs-26.3-x86_64\\bin\\emacsclientw.exe",
    "window.openWithoutArgumentsInNewWindow": "off"
